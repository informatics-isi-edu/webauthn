#!/usr/bin/python

import sys
import webauthn2

webauthn2_manager = webauthn2.Manager()

def main(args):
    try:
        webauthn2_manager.deploy()
    except Exception, te:
        print 'Deployment failed: %s' % str(te)
        return 1
    return 0

if __name__ == '__main__':
    sys.exit(main(sys.argv[1:]))

